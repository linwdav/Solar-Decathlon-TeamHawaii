			android:label="@string/Hvac" android:screenOrientation="portrait"
			android:configChanges="orientation|keyboardHidden" />
			
		<activity android:name="edu.hawaii.systemh.android.settings.Settings"
			android:label="@string/Settings" android:screenOrientation="portrait"
			android:configChanges="orientation|keyboardHidden" />

    <activity android:name="edu.hawaii.systemh.android.energy.Energy"
      android:label="@string/Energy" android:screenOrientation="portrait"
